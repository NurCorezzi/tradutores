void main() {
    int i; 
    int j;
    int it;
    it = 3;

    while(i != it + 1) {
        for (i = 2 ; i <= it; i = i + 1) {
            j = i;
            while(j) {
                << j;
                j = j - 1;
            }
            if (true) {
                << -1;
            }
        }
    }

    int[3] a;
    << a;
}

/*
2
1
-1
3
2
1
-1
[0, 0, 0, #]
*/